<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <LangVersion>7</LangVersion>
    <DefineConstants>TRYPARSERS_LIB</DefineConstants>
    <VersionPrefix>1.3.0</VersionPrefix>
    <Copyright>Copyright (c) 2012 Atif Aziz. All rights reserved.</Copyright>
    <Description>TryParse from .NET Framework done right</Description>
    <Authors>Atif Aziz</Authors>
    <Owners>Atif Aziz</Owners>
    <Company></Company>
    <PackageProjectUrl>https://github.com/atifaziz/TryParsers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/atifaziz/TryParsers</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>parser parsing linq functional</PackageTags>
    <PackageLicenseUrl>http://www.opensource.org/licenses/bsd-license.php</PackageLicenseUrl>
    <PackageOutputPath>dist</PackageOutputPath>
    <IncludeSymbols>true</IncludeSymbols>
    <IncludeSource>true</IncludeSource>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="**/*" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="COPYING.txt">
      <PackagePath />
    </Content>
  </ItemGroup>

</Project>
